<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.dtag.tlabs.wallet.tests.shopping"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".WalletTestShoppingActivity"
			android:screenOrientation="portrait"
            android:label="@string/title_activity_wallet_test_shopping" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

		<activity android:name="de.dtag.tlabs.wallet.extensions.shopping.shop.WalletShoppingEngine" >
			<intent-filter>
				<action android:name="de.dtag.tlabs.wallet.extensions.shopping.shop.WalletShoppingEngine" />

				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
    </application>

</manifest>